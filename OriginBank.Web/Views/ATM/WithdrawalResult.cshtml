@model OriginBank.Web.Models.WithdrawalResultViewModel

@{ ViewData["Title"] = "Resultado Retiro"; }

<h1>Retiro</h1>
<hr />

<div>
    <dl class="row">
        <dt class="col-md-6">
            Número de tarjeta
        </dt>
        <dd class="col-md-6">
            @Html.DisplayFor(model => model.Number)
        </dd>
        <dt class="col-md-6">
            Fecha operación
        </dt>
        <dd class="col-md-6">
            @Html.DisplayFor(model => model.CurrentTime)
        </dd>
        <dt class="col-md-6">
           Monto de retiro
        </dt>
        <dd class="col-md-6">
            @Html.DisplayFor(model => model.WithdrawalAmount)
        </dd>
        <dt class="col-md-6">
            Saldo
        </dt>
        <dd class="col-md-6">
            @Html.DisplayFor(model => model.RemainingBalance)
        </dd>
    </dl>
    <div class="row">
        <div class="col-md-4 margin-top-20"></div>
        <div class="col-md-4 col-md-offset-3 margin-top-20">
            <a href="Menu"><div class="btn btn-light btn-block margin-top-20">Atras</div></a>
            <a href="Index"><div class="btn btn-danger btn-block margin-top-20">Salir</div></a>
        </div>
    </div>
</div>